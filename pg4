#include <stdio.h>

int main() {
    int num1, num2;
    float quotient;

    printf("Enter dividend: ");
    scanf("%d", &num1);

    printf("Enter divisor: ");
    scanf("%d", &num2);

    if (num2 != 0) {
        quotient = (float) num1 / num2;
        printf("Quotient: %.2f\n", quotient);
    } else {
        printf("Cannot divide by zero.\n");
    }

    return 0;
}
