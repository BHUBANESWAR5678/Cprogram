#include <stdio.h>

int main() {
    int num1, num2, remainder;

    printf("Enter dividend: ");
    scanf("%d", &num1);

    printf("Enter divisor: ");
    scanf("%d", &num2);

    if (num2 != 0) {
        remainder = num1 % num2;
        printf("Remainder: %d\n", remainder);
    } else {
        printf("Cannot calculate remainder with zero divisor.\n");
    }

    return 0;
}
